{
  "name": "starter-api",
  "version": "0.0.1",
  "scripts": {
    "dev": "bun run --watch src/index.ts | pino-pretty",
    "dev:telemetry": "bun run --preload ./src/telemetry.ts --watch src/index.ts | pino-pretty",
    "start": "bun run --watch src/index.ts",
    "start:telemetry": "bun run --preload ./src/telemetry.ts --watch src/index.ts",
    "test": "bun test --coverage",
    "test:watch": "bun test --watch",
    "lint": "bunx @biomejs/biome check --apply src test drizzle",
    "migration:make": "drizzle-kit generate:pg",
    "migration:up": " export $(cat .env | xargs) && bun run drizzle/migrate.ts",
    "migration:studio": "drizzle-kit studio",
    "migration:seed": "export $(cat .env | xargs) && bun run drizzle/seed.ts | pino-pretty",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": "bunx @biomejs/biome check --apply"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/graphql-yoga": "^1.0.2",
    "@elysiajs/server-timing": "^1.0.2",
    "@elysiajs/stream": "^1.0.2",
    "@elysiajs/swagger": "^1.0.3",
    "@faker-js/faker": "^8.4.1",
    "@t3-oss/env-core": "^0.9.2",
    "argon2": "^0.40.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.6",
    "elysia": "^1.0.11",
    "elysia-auth-drizzle": "^1.1.2",
    "elysia-helmet": "^1.0.2",
    "elysia-rate-limit": "^3.1.1",
    "graceful-server-elysia": "^1.0.4",
    "graphql-rate-limit-directive": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pino": "^8.19.0",
    "reflect-metadata": "^0.2.2",
    "timestring": "^7.0.0",
    "type-graphql": "2.0.0-beta.6",
    "unify-elysia": "^1.0.9",
    "unify-elysia-gql": "^1.0.5",
    "unify-errors": "1.2.229",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.4",
    "@commitlint/config-conventional": "^19.1.0",
    "@opentelemetry/auto-instrumentations-node": "^0.43.0",
    "@opentelemetry/exporter-zipkin": "^1.22.0",
    "@opentelemetry/sdk-node": "^0.49.1",
    "@types/jsonwebtoken": "^9.0.6",
    "bun-types": "latest",
    "drizzle-kit": "^0.20.14",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "pg": "^8.11.4",
    "pino-pretty": "^11.0.0",
    "postgres": "^3.4.4",
    "tsup": "^8.0.2"
  },
  "module": "src/index.js"
}