{
  "name": "starter-api",
  "version": "0.0.1",
  "scripts": {
    "dev": "bun run --watch src/index.ts | pino-pretty",
    "dev:telemetry": "bun run --preload ./src/telemetry.ts --watch src/index.ts | pino-pretty",
    "start": "bun run --watch src/index.ts",
    "start:telemetry": "bun run --preload ./src/telemetry.ts --watch src/index.ts",
    "test": "bun test --coverage",
    "test:watch": "bun test --watch",
    "lint": "eslint src",
    "migration:make": "drizzle-kit generate:pg",
    "migration:up": " export $(cat .env | xargs) && bun run drizzle/migrate.ts",
    "migration:studio": "drizzle-kit studio"
  },
  "resolutions": {
    "graphql": "^15.3.0"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/graphql-yoga": "^1.0.2",
    "@elysiajs/server-timing": "^1.0.2",
    "@elysiajs/swagger": "^1.0.3",
    "@t3-oss/env-core": "^0.9.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.6",
    "elysia": "^1.0.11",
    "elysia-auth-drizzle": "^1.0.3",
    "elysia-helmet": "^1.0.2",
    "elysia-rate-limit": "^3.1.1",
    "graceful-server-elysia": "^1.0.4",
    "graphql-rate-limit-directive": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pino": "^8.19.0",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "1.1.1",
    "unify-elysia": "^1.0.9",
    "unify-elysia-gql": "^1.0.4",
    "unify-errors": "1.2.229",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.43.0",
    "@opentelemetry/exporter-zipkin": "^1.22.0",
    "@opentelemetry/sdk-node": "^0.49.1",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "pg": "^8.11.4",
    "pino-pretty": "^11.0.0",
    "postgres": "^3.4.4",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2"
  },
  "module": "src/index.js"
}