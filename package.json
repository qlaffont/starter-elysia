{
  "name": "starter-api",
  "version": "0.0.1",
  "scripts": {
    "dev": "bun run --watch src/index.ts | pino-pretty",
    "dev:telemetry": "bun run --preload ./src/telemetry.ts --watch src/index.ts | pino-pretty",
    "start": "bun run --watch src/index.ts",
    "start:telemetry": "bun run --preload ./src/telemetry.ts --watch src/index.ts",
    "test": "bun test --coverage",
    "test:watch": "bun test --watch",
    "lint": "bunx @biomejs/biome check --apply src test drizzle && bunx tsc --noEmit",
    "migration:make": "drizzle-kit generate",
    "migration:up": " export $(cat .env | xargs) && bun run drizzle/migrate.ts",
    "migration:studio": "drizzle-kit studio",
    "migration:seed": "export $(cat .env | xargs) && bun run drizzle/seed.ts | pino-pretty",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": "bunx @biomejs/biome check --apply"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/graphql-yoga": "^1.0.3",
    "@elysiajs/server-timing": "^1.0.2",
    "@elysiajs/stream": "^1.0.2",
    "@elysiajs/swagger": "^1.0.4",
    "@faker-js/faker": "^9.0.0",
    "@graphql-tools/schema": "^10.0.3",
    "@t3-oss/env-core": "^0.11.0",
    "argon2": "^0.43.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.0",
    "elysia": "^1.0.16",
    "elysia-auth-drizzle": "^1.1.16",
    "elysia-helmet": "^2.0.0",
    "elysia-rate-limit": "^4.0.0",
    "graceful-server-elysia": "^1.0.4",
    "graphql-rate-limit-directive": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.7",
    "pino": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "timestring": "^7.0.0",
    "type-graphql": "2.0.0-rc.2",
    "unify-elysia": "^1.1.0",
    "unify-elysia-gql": "^1.1.0",
    "unify-errors": "^1.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/config-conventional": "^19.1.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-zipkin": "^1.23.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@types/jsonwebtoken": "^9.0.6",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.0",
    "husky": "^9.0.11",
    "lint-staged": "^16.0.0",
    "pg": "^8.11.5",
    "pino-pretty": "^13.0.0",
    "postgres": "^3.4.4",
    "tsup": "^8.0.2"
  },
  "module": "src/index.js",
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
